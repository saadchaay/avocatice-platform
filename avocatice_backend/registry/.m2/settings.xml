<settings
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
	xmlns="http://maven.apache.org/SETTINGS/1.1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<servers>
		<server>
			<id>central</id>
			<username>${env.OC_ACCOUNT_SIRHUS}</username>
			<password>${env.OC_SECRET_SIRHUS}</password>
		</server>
		<server>
			<id>releases</id>
			<username>${env.OC_ACCOUNT_SIRHUS}</username>
			<password>${env.OC_SECRET_SIRHUS}</password>
		</server>
		<server>
			<id>snapshots</id>
			<username>${env.OC_ACCOUNT_SIRHUS}</username>
			<password>${env.OC_SECRET_SIRHUS}</password>
		</server>
	</servers>

	<profiles>
		<profile>
			<activation>
				<activeByDefault>true</activeByDefault>
			</activation>
			<repositories>
				<repository>
					<id>central</id>
					<url>https://repos.tech.orange/artifactory/maven-proxy-asis-maven2-releases/
					</url>
				</repository>
				<repository>
					<id>release</id>
					<url>https://repos.tech.orange/artifactory/sirhus-virt-maven-stable
					</url>
				</repository>
				<repository>
					<id>snapshots</id>
					<url>https://repos.tech.orange/artifactory/sirhus-virt-maven-unstable
					</url>
				</repository>
			</repositories>
			<pluginRepositories>
				<pluginRepository>
					<id>central</id>
					<url>https://repos.tech.orange/artifactory/maven-proxy-asis-maven2-releases/
					</url>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>
</settings> 
